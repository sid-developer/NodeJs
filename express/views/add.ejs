<!DOCTYPE html>
<html lang="en">

  <%- include('./partials/head') %>

        
<body>
    
    <%- include('./partials/nav') %>

    <div class="container">
      
        <div class="row">
          <form class="col s12" action="/save-data" method="POST">
            <div class="row">
              <div class="input-field col s4">
                <i class="material-icons prefix">account_circle</i>
                <input id="icon_prefix" type="text" class="validate" name="first_name">
                <label for="icon_prefix">First Name</label>
              </div>
              <div class="input-field col s4">
                <i class="material-icons prefix">account_circle</i>
                <input id="icon_prefix" type="text" class="validate" name="last_name">
                <label for="icon_prefix">Last Name</label>
              </div>
              <div class="input-field col s4">
                <i class="material-icons prefix">phone</i>
                <input id="icon_telephone" type="tel" class="validate" name="phone">
                <label for="icon_telephone">Telephone</label>
              </div>
              <div class="input-field col s4">
                <button type="submit" class="waves-effect waves-light btn">Submit</button>
              </div>
              
            </div>
          </form>
        </div>
        

    </div>

    
          
</body>
</html>